package i.vk.exp;

import java.time.LocalDateTime;

import i.vk.inders.ExpPacket;

public class CourseNotFoundException extends RuntimeException {
	public CourseNotFoundException() {
		
	}
	public ExpPacket courseNotFound() {
		ExpPacket exp = new ExpPacket();
		exp.setExpCode("ERK_CNF:88101");
		exp.setExpMsg("Course Not Found");
		exp.setOccuredOn(LocalDateTime.now());
	}
}
